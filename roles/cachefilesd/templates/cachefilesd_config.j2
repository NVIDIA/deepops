dir {{ cachefilesd_cache_dir }}
{% if cachefilesd_cache_tag %}
tag {{ cachefilesd_cache_tag }}
{% endif %}
brun 25%
bcull 10%
bstop 3%
frun 10%
fcull 7%
fstop 3%

# Assuming you're using SELinux with the default security policy included in
# this package
secctx system_u:system_r:cachefiles_kernel_t:s0
